@model List<mypro.Conrext.Student>


@{
    ViewBag.Title = "StudentList";
}
<h2 class="text-center">All Student</h2>
<table class="table table-hover top ">
   
    <tr>
        <th>Name</th>
        <th>Fname</th>
        <th>Email</th>
        <th>Mobile</th>
        @*<th>College</th>*@
        <th>Description</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Fname</td>
            <td>@item.Email</td>
            <td>@item.Mobile</td>
            @*<td>@item.College</td>*@
            <td>@item.Description</td>
            <td>
                <a href="@Url.Action("Student",new { item.ID,item.Name, item.Fname, item.Email, item.Mobile, item.Description, })" class="btn btn-primary">Edit</a>
                @*<a href="@Url.Action("Details",new { item.ID,item.Name, item.Fname, item.Email, item.Mobile, item.Description, })" class="btn btn-primary">Details</a>*@
                @*@Html.ActionLink("Details", "Details", new { id = item.ID , @class = "btn btn-primary"})*@ 
                <a href="@Url.Action("Details",new { id = item.ID })" class="btn btn-info">Details</a>
                <a href="@Url.Action("Delete",new { item.ID})" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>

